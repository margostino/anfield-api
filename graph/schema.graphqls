type Team {
  id: ID!
  name: String!
  shortName: String!
	strengthOverallHome: Int!,
  strengthOverallAway: Int!,
  strengthAttackHome: Int!,
  strengthAttackAway: Int!,
  strengthDefenceHome: Int!,
  strengthDefenceAway: Int!,
}

type Player {
  id: ID!
  firstName: String!
  secondName: String!
  webName: String!
  news: String!
  newsAdded: String!
  team: String!
}

type Query {
  team(shortName:String!): Team
  teams: [Team]
  player(webName:String!): Player  
}