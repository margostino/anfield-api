type Team {
  id: ID!
  name: String!
  shortName: String!
}

type Player {
  id: ID!
  teamId: ID!
  firstName: String!
  secondName: String!
  webName: String!
  news: String!
  newsAdded: String!
  team: Team!
}

type Query {
  team(shortName:String!): Team
  teams: [Team]
  player(webName:String!): Player  
}